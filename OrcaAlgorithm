def OperateOrca(path_from, path_to_ndump2):
    dir = os.listdir(folder_from)

    for file in dir:
        name_without_end = file.replace(".in" ,"")
        if name_without_end +".ndump2" in os.listdir(ndump2_folder):
            continue
        print(name_without_end)
        print("orca.exe node 4 " + file   + ndump2_folder + '/' + name_without_end +".ndump2")
        subprocess.call("orca.exe node 4 "+ path_from + ' '+ path_to_ndump2+".ndump2")
     
